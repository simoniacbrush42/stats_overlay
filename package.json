{
  "name": "stats_overlay",
  "productName": "Foresight Overlay",
  "version": "1.0.1",
  "description": "A stats overlay for many gamemodes on hypixel",
  "main": "src/index.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build": "electron-builder build --mac --win --publish never",
    "deploy": "electron-builder build --mac --win --publish always"
  },
  "build": {
    "appId": "stats_overlay",
    "productName": "Foresight Overlay",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/icon.ico"
    },
    "mac": {
      "icon": "src/mac-icon.icns",
      "category": "public.app-category.developer-tools",
      "type": "distribution"
    },
    "nsis": {
      "installerIcon": "src/icon.ico",
      "uninstallerIcon": "src/icon.ico",
      "uninstallDisplayName": "Foresight Overlay",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "publish": [
      "github"
    ]
    
  },
  "keywords": [],
  "author": {
    "name": "simoniacbrush42",
    "email": "simoniacbrush42@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "stats_overlay"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "auto-git-update": "^1.1.1",
    "chokidar": "^3.5.3",
    "electron-json-storage": "^4.5.0",
    "electron-settings": "^4.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^5.0.5",
    "electron-window-position": "^1.0.1",
    "findit": "^2.0.0",
    "fs": "^0.0.1-security",
    "hypixie": "^1.0.0",
    "log-timestamp": "^0.3.0",
    "md5": "^2.3.0",
    "node-fetch": "^2.6.1",
    "node-watch": "^0.7.1",
    "path": "^0.12.7",
    "read-last-lines": "^1.8.0",
    "tail": "^2.2.4",
    "text-file-follower": "^0.1.0",
    "uaup-js": "^0.0.22",
    "update-electron-app": "^2.0.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "electron": "15.5.5",
    "electron-builder": "^23.1.0"
  }
}
